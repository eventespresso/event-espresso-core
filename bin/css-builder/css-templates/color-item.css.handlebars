/** {{ colorLabel }} **/
.ee-{{color}}-background-color {
    background-color: var(--ee-color-{{color}}, {{main_color}});
}
.ee-{{color}}-background-color.with-accessible-foreground {
    color: var( --ee-color-{{color}}-foreground-contrast, {{foreground_contrast}} );
}
.ee-{{color}}-background-color.with-accessible-foreground.with-hover:hover,
.ee-{{color}}-background-color.with-linear-gradient.with-accessible-foreground.with-hover:hover,
.ee-{{color}}-background-color.with-linear-gradient.with-hover:hover,
.ee-{{color}}-background-color.with-hover:hover {
    background-image: none;
    background-color: var( --ee-color-{{color}}-hover-contrast, {{hover_contrast}} );
}
.ee-{{color}}-color {
    color: var(--ee-color-{{color}}, {{main_color}});
}
.ee-{{color}}-color.with-accessible-background {
    background-color: var(--ee-color-{{color}}-background-contrast, {{foreground_contrast}} );
}
.ee-{{color}}-color.with-accessible-background.with-hover:hover {
    background-color: var(--ee-color-{{color}}-hover, {{hover}});
}
.ee-{{color}}-background-color.with-linear-gradient {
    background-image: linear-gradient(
        var(--ee-{{color}}-background-gradient-x, {{gradient_x}}),
        var(--ee-{{color}}-background-gradient-y, {{gradient_y}})
    );
}
.ee-{{color}}-background-color.with-linear-gradient.with-accessible-foreground {
    color: var(--ee-color-{{color}}-background-contrast, {{background_contrast}} );
}
