:root {
    {{#each sizeItems}}
    {{{this}}}
    {{/each}}

    /** font size names **/
    --ee-font-size-small: {{sizeSmall}};
    --ee-font-size-medium: {{sizeMedium}};
    --ee-font-size-large: {{sizeLarge}};
    --ee-font-size-huge: {{sizeHuge}};

    /** radius size names **/
    --ee-radius-size-small: {{radiusSmall}};
    --ee-radius-size-normal: {{radiusNormal}};
}