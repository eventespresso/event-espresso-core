/** {{ colorLabel }} **/
    --ee-color-{{color}}: {{main_color}};
    --ee-color-{{color}}-background-contrast: {{background_contrast}};
    --ee-color-{{color}}-foreground-contrast: {{foreground_contrast}};
    --ee-{{color}}-background-gradient-x: {{gradient_x}};
    --ee-{{color}}-background-gradient-y: {{gradient_y}};
    --ee-color-{{color}}-hover: {{hover}};
    --ee-color-{{color}}-hover-contrast: {{hover_contrast}};
    --ee-color-{{color}}-01-transparency: rgba( {{rgba1}},{{rgba2}},{{rgba3}},0.01 );
    --ee-color-{{color}}-25-transparency: rgba( {{rgba1}},{{rgba2}},{{rgba3}},0.25 );
    --ee-color-{{color}}-50-transparency: rgba( {{rgba1}},{{rgba2}},{{rgba3}},0.50 );
    --ee-color-{{color}}-75-transparency: rgba( {{rgba1}},{{rgba2}},{{rgba3}},0.75 );
