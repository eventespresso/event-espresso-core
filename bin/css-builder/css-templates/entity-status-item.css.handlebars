/** {{ status_label }} **/
    .ee-status-color-{{status_code}} {
        color: var(--ee-color-{{ color }}, {{main_color}});
    }
    .ee-status-color-{{status_code}}.with-accessible-background {
        background-color: var(--ee-color-{{ color }}-background-contrast, {{background_contrast}})
    }
    .ee-status-color-{{status_code}}.with-accessible-background.with-hover:hover {
        background-color: var(--ee-color-{{ color }}-hover, {{hover}})
    }
    .ee-status-background-color-{{status_code}} {
        background-color: var(--ee-color-{{ color }}, {{main_color}});
    }
    .ee-status-background-color-{{status_code}}.with-accessible-foreground {
        color: var( --ee-color-{{ color }}-foreground-contrast, {{foreground_contrast}} );
    }
    .ee-status-background-color-{{status_code}}.with-accessible-foreground.with-hover:hover {
        background-color: var( --ee-color-{{ color }}-hover-contrast, {{hover_contrast}} );
    }
