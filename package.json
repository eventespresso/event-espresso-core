{
	"name": "@eventespresso/core",
	"version": "1.0.0",
	"author": "Event Espresso",
	"license": "GPL-3.0",
	"description": "Event Espresso core js and css.",
	"main": "config/webpack.dev.js",
	"repository": "git+https://github.com/eventespresso/event-espresso-core.git",
	"watch": {
		"build:css": {
			"patterns": [
				"bin/css-builder",
				"assets/ZZZ/components/ui/styles"
			],
			"ignore": "index.js"
		}
	},
	"scripts": {
		"build": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.js",
		"build:css": "node ./bin/css-builder/css-builder.js",
		"check-engines": "wp-scripts check-engines",
		"ci:build": "npm run build",
		"ci:lint": "npm run lint",
		"ci:test": "npm run test-js",
		"dev": "concurrently \"webpack --config ./config/webpack.dev.js\" \"npm run build:css\"",
		"dev-w": "concurrently \"webpack --watch --config ./config/webpack.dev.js\" \"npm-watch build:css\"",
		"dev-p": "webpack --watch --watch-poll --config ./config/webpack.dev.js",
		"lc": "jsgl --local ./",
		"lint": "wp-scripts lint-js .",
		"lint:fix": "wp-scripts lint-js . --fix",
		"lint-js": "wp-scripts lint-js .",
		"lint-js:fix": "wp-scripts lint-js . --fix",
		"lint-php": "composer run-script lint",
		"lint-php:fix": "composer run-script lint-fix",
		"lint-php:skip-warnings": "composer run-script lint:skip-warnings",
		"prebuild": "npm run check-engines && npm run build:css",
		"pretty": "prettier --write \"assets/prototype/**/*.js\"",
		"prewatch": "npm run check-engines",
		"test": "npm run lint && npm run test-js",
		"test-php": "cd tests && phpunit",
		"test-js": "wp-scripts test-unit-js --config tests/javascript-config/unit/jest.config.json",
		"test-js-ts": "jest --config config/tests/unit/jest.config.js",
		"test-js-ts:watch": "jest --config config/tests/unit/jest.config.js --watch",
		"test-js:coverage": "npm run test-js -- --coverage",
		"test-js:coverage-ci": "npm run test-js -- --coverage --maxWorkers 1",
		"test-js:update": "npm run test-js -- --updateSnapshot",
		"test-js:watch": "npm run test-js -- --watch"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"assets/ZZZ/**/*.js": [
			"wp-scripts lint-js"
		]
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"dependencies": {
		"@apollo/react-hooks": "^3.1.3",
		"@blueprintjs/core": "^3.17.2",
		"@blueprintjs/datetime": "^3.15.1",
		"@blueprintjs/select": "^3.11.2",
		"@eventespresso/react-exit-modal-typeform": "^1.0.4",
		"@wordpress/i18n": "^3.7.0",
		"@wordpress/is-shallow-equal": "^1.6.1",
		"accounting-js": "^1.1.1",
		"apollo-cache-inmemory": "^1.6.3",
		"apollo-client": "^2.6.4",
		"apollo-link-http": "^1.5.16",
		"classnames": "^2.2.6",
		"cuid": "^2.1.6",
		"date-fns": "^2.8.1",
		"date-fns-tz": "^1.0.8",
		"decimal.js-light": "^2.5.0",
		"final-form": "^4.18.6",
		"final-form-arrays": "^3.0.2",
		"final-form-calculate": "^1.3.1",
		"graphql": "^14.5.8",
		"graphql-tag": "^2.10.1",
		"immutable": "^4.0.0-rc.12",
		"js-green-licenses": "^1.1.0",
		"memize": "^1.0.5",
		"moment": "^2.24.0",
		"moment-duration-format": "^2.3.2",
		"moment-timezone": "^0.5.27",
		"npm-watch": "^0.6.0",
		"pluralize": "^8.0.0",
		"prop-types": "^15.7.2",
		"querystringify": "^2.1.1",
		"ramda": "^0.26.1",
		"react-beautiful-dnd": "^11.0.4",
		"react-currency-formatter": "^1.1.0",
		"react-final-form": "^6.3.3",
		"react-final-form-arrays": "^3.1.1",
		"react-final-form-listeners": "^1.0.2",
		"react-js-pagination": "^3.0.2",
		"react-select": "^3.0.8",
		"react-spring": "^8.0.27",
