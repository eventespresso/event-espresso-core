{
	"name": "@eventespresso/core",
	"version": "1.0.0",
	"author": "Event Espresso",
	"license": "GPL-3.0",
	"description": "Event Espresso core js and css.",
	"main": "config/webpack.dev.js",
	"repository": "git+https://github.com/eventespresso/event-espresso-core.git",
	"watch": {
		"build:css": {
			"patterns": [
				"bin/css-builder",
				"assets/ZZZ/components/ui/styles"
			],
			"ignore": "index.js"
		}
	},
	"scripts": {
		"build:css": "node ./bin/css-builder/css-builder.js",
		"ci:build": "npm run prod",
		"ci:test": "npm run test-js",
		"dev": "node scripts/watch.js",
		"generate": "graphql-codegen",
		"lc": "jsgl --local ./",
		"lint-php": "composer run-script lint",
		"lint-php:fix": "composer run-script lint-fix",
		"lint-php:skip-warnings": "composer run-script lint:skip-warnings",
		"pretty": "prettier --write \"assets/src/**/*.js\"",
		"prod": "node scripts/build.js",
		"test": "npm run lint && npm run test-js",
		"test-php": "cd tests && phpunit",
		"test-js": "node scripts/test.js",
		"test-js:coverage": "npm run test-js -- --coverage",
		"test-js:coverage-ci": "npm run test-js -- --coverage --maxWorkers 1"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"assets/src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --write",
			"git add"
		]
	},
	"babel": {
		"presets": [
			"react-app"
		]
	},
